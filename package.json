{
  "name": "ramverk2",
  "version": "2.0.3",
  "description": "Me-page with express and react",
  "main": "react-frontend/index.js",
  "scripts": {
    "start": "node express-backend/index.js",
    "start-client": "cd react-frontend && npm start",
    "start-backend": "node express-backend/index.js",
    "start-dev": "concurrently --kill-others \"npm run start-client\" \"npm run start-backend\"",
    "install": "cd react-frontend && npm install && npm run build",
    "test": "cd react-frontend && npm test && npm test -- --coverage",
    "test1": "docker-compose run node7 && npm test",
    "test2": "docker-compose run node8 && npm test",
    "test3": "docker-compose run node9 && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcusgsta/ramverk2.git"
  },
  "author": "Marcus Gustafsson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcusgsta/ramverk2/issues"
  },
  "homepage": "https://github.com/marcusgsta/ramverk2#readme",
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "clean-css-cli": "^4.1.10",
    "codecov": "^3.0.0",
    "coveralls": "^3.0.0",
    "csslint": "^1.0.5",
    "eslint": "^4.10.0",
    "eslint-config-postcss": "^2.0.2",
    "eslint-plugin-pug": "^1.1.0",
    "eslint-plugin-react": "^7.4.0",
    "gulp-postcss": "^7.0.0",
    "html-minifier": "^3.5.6",
    "htmlhint": "^0.9.13",
    "js-yaml": "^3.10.0",
    "jscs": "^3.0.7",
    "jsonlint": "^1.6.2",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "postcss-reporter": "^5.0.0",
    "postcss-scss": "^1.0.2",
    "stylelint": "^8.2.0",
    "stylelint-config-recommended-scss": "^2.0.0",
    "stylelint-scss": "^2.1.0",
    "ajv": "5.0.3-beta.0"
  },
  "dependencies": {
    "concurrently": "^3.5.0",
    "express": "^4.16.2",
    "jstransformer-markdown-it": "^2.0.0",
    "pug": "^2.0.0-rc.4",
    "stylelint-config-sass-guidelines": "^3.1.0"
  }
}
